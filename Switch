#include <stdio.h>

int main()
{
 int n;
  printf("Enter a number from 1 to 5");
  scanf("%d,",&n);
  switch (n)
  {
    case 1:printf("Pizza\n Rs 239");
    break;
    case 2:printf("Burger \n Rs 129");
    break;
    case 3:printf("Pasta \n Rs 179");
    break;
    case 4:printf("French fries \n Rs 99");
    break;
    case 5:printf("Sandwich \n Rs 149");
    break;
    default:printf("You have entered wrong number ");
    
  }
    
  
  return 0;
}
